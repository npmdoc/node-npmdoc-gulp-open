{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "gulp-open",
    "description": "#### basic api documentation for  [gulp-open (v2.0.0)](http://github.com/stevelacy/gulp-open)  [![npm package](https://img.shields.io/npm/v/npmdoc-gulp-open.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-gulp-open) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-gulp-open.svg)](https://travis-ci.org/npmdoc/node-npmdoc-gulp-open)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "gulp-open": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-gulp-open",
    "keywords": [
        "documentation",
        "gulp-open"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_gulp_open.js",
    "name": "npmdoc-gulp-open",
    "nameAlias": "npmdoc_gulp_open",
    "nameOriginal": "npmdoc-gulp-open",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-gulp-open.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "2017.4.27"
}
